/* pages/coffeeWallet/coffeeWallet.wxss */
page {
  width: 100%;
  height: 100%;
  background: #f6f6f6;
  display: flex;
  display: -webkit-flex;
  flex-direction: column;
  justify-content: flex-start;
}
.container{
  width: 100%;
  height: 100%;
  display: flex;
  display: -webkit-flex;
  flex-direction: column;
}
.header{
  height: 5%;
  width: 100%;
}
.section{
  width: 100%;
  height: 85%;
  display: flex;
  display: -webkit-flex;
  flex-direction: column;
}
.footer{
  width: 100%;
  height: 10%;
  display: flex;
  display: -webkit-flex;
  flex-direction: row;
  padding: 20rpx 0rpx;
  box-sizing: border-box;
}
.empty{
  width: 100%;
  height: 100%;
  display: flex;
  display: -webkit-flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.empty>image{
width: 150rpx;
height: 150rpx;
}
.empty text{
  font-size: 24rpx;
  color: #636363	;
  margin-top: 50rpx;
}
.footer>view{
  width: 33%;
  height: 100%;
  border-left: 1rpx #636363 solid;
  display: flex;
  display: -webkit-flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 24rpx;
  color: #636363;
  }
  .footer>view:nth-child(1){
border: none;
  }
  .footer image{
    width: 60rpx;
    height: 60rpx;
  }
