/* pages/address/address.wxss */
page {
  width: 100%;
  height: 100%;
  background: #f6f6f6;
  display: flex;
  display: -webkit-flex;
  flex-direction: column;
  justify-content: flex-start;
  font-size: 32rpx;
}
.container {
  width: 100%;
  height: 100%;
  display: flex;
  display: -webkit-flex;
  flex-direction: column;
}
.zeroAddress{
  width: 100%;
  height: 90%;
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.zeroAddress image{
  margin-top: 30%;
  width: 200rpx;
  height: 200rpx;
}
.zeroAddress text{
  color: #7d7371;
  margin-top: 60rpx;
}
.footer{
  width: 100%;
  height: 10%;
  display: flex;
  display: -webkit-flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  color: #4a86e8;
}
