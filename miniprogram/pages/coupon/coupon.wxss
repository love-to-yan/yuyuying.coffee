/* pages/coupon/coupon.wxss */
page {
  width: 100%;
  height: 100%;
  background: #f6f6f6;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
.container{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}
::-webkit-scrollbar{
  width: 0rpx;
  height: 0rpx;
  color: transparent;
}
.main{
  width: 100%;
  display: flex;
  display: -webkit-flex;
  flex-direction: column;
  padding: 10rpx;
  box-sizing: border-box;
  border: 1rpx #96CDCD solid;
}